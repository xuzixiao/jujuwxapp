.active-list{
  padding: 20px 20px 10px;
  height: 180px;
  background: #fff;
  border-radius: 5px;
  box-shadow: 0px 0px 5px rgba(0,0,0,.1);
  margin-bottom: 10px;
}
.active-logo{
  width: 100%;
  height: 108px;
  display: flex;
  overflow: hidden;
  justify-items:center;
  border-radius: 5px;
  position: relative;
}
.active-logo image{
  width: 100%;
  height: 108px;
}
.activetit,.activeaddress,.activedate{
  font-size: 14px;
  color: #666666;
  line-height: 28px;
  width: 100%;
  height: 28px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
} 
.activeaddress,.activedate{
  height: 22px!important;
  line-height: 22px!important;
  color: rgba(102,102,102,0.80);
}
.icon{
  display:block;
  width: 15px;
  height: 15px;
  margin: 3px 3px 0px 0px;
  float: left;
}
.tag{
  position: absolute;
  top: 10px;
  left: 10px;
  border-radius: 2px;
  color: #fff;
  font-size: 12px;
  line-height: 20px;
  padding: 1px 5px;
}
.ing{
   background: rgba(245,33,45,0.80);
}
.begin{
  background: rgba(0,0,0,0.52);
}
.after{
  background: rgba(144,144,144,0.80);
}