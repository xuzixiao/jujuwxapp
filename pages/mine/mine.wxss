.mine-box{
  width: 94%;
  margin: 150rpx 3% 30px;
  background: #fff;
  box-shadow: 2rpx 2rpx 20rpx rgba(0,0,0,.1);
  border-radius: 10rpx;
}
.mine-head{
  width: 100%;
  height: 280rpx;
  float: left;
  margin-top: -75rpx;
  border-bottom: #eaeef1 solid 1rpx;
}
.headimg{
  display: block;
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  margin: 0 auto;
  box-shadow: 0px 0px 20px rgba(0,0,0,.1);
}
.nickname{
  display: block;
  width: 100%;
  height:100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 40rpx;
  color: #666666;
}
.mine-nav{
  width: 94%;
  height: auto;
  float: left;
  padding-left: 6%;
  border-bottom: #eaeef1 solid 1px;
  margin-bottom:150rpx;
}
.mine-nav navigator{
  height: 80rpx;
  border-bottom: #eaeef1 solid 1px;
}
.mine-nav navigator:last-child{
  border-bottom: none;
}
.mine-nav navigator .label{
  background: #35cfc8;
  width: 10rpx;
  height: 30rpx;
  margin-top: 25rpx;
  float: left;
}
.mine-nav navigator text{
  font-size: 30rpx;
  line-height: 80rpx;
  margin-left: 20rpx;
  color: #4f5961;
}
.mine-nav navigator image{
  width: 30rpx;
  height: 30rpx;
  float: right;
  margin: 25rpx;
}

