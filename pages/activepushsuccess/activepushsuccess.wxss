.pushsuccess image{
  display: block;
  width: 130px;
  height: 100px;
  margin: 120px auto 10px;
}
.line-success-text{
  text-align: center;
  color: #35CFC9;
  font-size: 20px;
  line-height: 28px;
  margin: 20px 0 10px;
}
.line-tip{
  font-size: 13px;
  color: #97A9B8;
  text-align: center;
}
.btn{
  margin: 40px 80px;
}
.btn button{
  margin-bottom: 10px;
  height: 30px;
  line-height: 30px;
  font-size: 14px;
  border: none;
  box-shadow: 0px 0px 5px rgba(0, 0, 0, .1);
}
.share{
background-image: linear-gradient(-180deg, #5CDBD3 0%, #35CFC9 99%);
box-shadow: 0 0 10px 0 rgba(0,109,117,0.20);
border-radius: 4px;
color: #fff;
}
.backindex{
  background: #FFFFFF;
  box-shadow: 0 0 10px 0 rgba(0,109,117,0.20);
  border-radius: 4px;
  color: #35CFC9;
}





