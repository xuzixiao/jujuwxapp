.feedback{
  width: 92%;
  padding: 10rpx 4% 0rpx;
  border-top: #f2f4f6 solid 1px;
}
.feedback textarea{
  line-height: 60rpx;
  font-size: 30rpx;
  color: #919191;
  height: 240rpx;
  margin-top:10rpx; 
}
.fontsize{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  text-align: right;
  font-size: 32rpx;
  color: #919191;
}
.submit{
  height: 86rpx;
  line-height: 86rpx;
  font-size: 36rpx;
  color: #fff;
  border: none;
  background: #35cfc9;
  margin-top: 40rpx;
}
.save{
  font-size: 40rpx;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  color: #35CFC9;
  background: #FFFFFF;
  box-shadow: 0 -2px 10px 0 rgba(0,109,117,0.10);
  position: fixed;
  left: 0px;
  bottom: 0px;
  border:none!important;

}