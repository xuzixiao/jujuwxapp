/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.clear{
  clear: both;
}
.save{
  font-size: 40rpx;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  color: #35CFC9;
  background: #FFFFFF;
  box-shadow: 0 -2px 10px 0 rgba(0,109,117,0.10);
  position: fixed;
  left: 0px;
  bottom: 0px;
  border:none;
}
